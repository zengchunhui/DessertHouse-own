<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">
    
    <struts>
    		<package name="index" namespace="/api" extends="json-default">
    			<action name="AddCart" class="addCart" method="execute">
    				<result type="json">
    					<param name="root">jsonResult</param>
    				</result>
    			</action>
    			<action name="ChangeCart" class="changeCart" method="execute">
    				<result type="json">
    					<param name="root">jsonResult</param>
    				</result>
    			</action>
    			<action name="GetCart" class="getCart" method="execute">
    				<result type="json">
    					<param name="root">jsonResult</param>
    				</result>
    			</action>
    			<action name="Reservate" class="reservate" method="execute">
    				<result type="json">
    					<param name="root">jsonResult</param>
    				</result>
    			</action>
    		</package>		
    </struts>